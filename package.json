{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.12",
    "axios": "^0.21.0",
    "date-fns": "^2.16.1",
    "graphcms-image": "^1.1.0-beta4",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.10.1",
    "jest-coverage-badges": "^1.1.2",
    "react": "^17.0.1",
    "react-apollo": "^3.1.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-scroll": "^1.8.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-scroll": "^1.8.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "enzyme-to-json": "^3.3.5",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**",
      "src/App.tsx",
      "!src/index.tsx"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "json-summary"
    ],
    "modulePathIgnorePatterns": [
      "npm-cache",
      ".npm"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg|jpg|png)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "jest --outputFile=TestResults.json --coverage --runInBand",
    "test:watch": "yarn test -- --watch",
    "test:update": "yarn test -- -u",
    "test:coverage": "yarn test -- --coverage",
    "test:badges": "yarn run test:coverage  && jest-coverage-badges",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
